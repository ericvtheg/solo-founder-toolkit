{
  "description": "Auto-documentation system for tracking metrics and user feedback",
  "hooks": {
    "PostToolUse": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "prompt",
            "prompt": "${CLAUDE_PLUGIN_ROOT}/hooks/prompts/detect-documentation.md",
            "timeout": 30
          }
        ]
      }
    ]
  }
}
